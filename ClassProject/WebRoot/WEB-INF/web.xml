  </servlet-mapping>
     <servlet-mapping>
    <servlet-name>courseServlet</servlet-name>
    <url-pattern>/adminDelcourse</url-pattern>
  </servlet-mapping>
     <servlet-mapping>
    <servlet-name>courseServlet</servlet-name>
