  </servlet>
  <servlet>
    <servlet-name>User</servlet-name>
    <servlet-class>com.lb.sev.User</servlet-class>
  </servlet>





	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>courseServlet</servlet-name>
    <url-pattern>/courseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>courseServlet</servlet-name>
    <url-pattern>/addcourseServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>courseServlet</servlet-name>
